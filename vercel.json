{
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "builds": [
    {
      "src": "api/main.py",
      "use": "@vercel/python"
    }
  ],
  "rewrites": [
    { "source": "/api/:path*", "destination": "/api/main.py" },
    { "source": "/health", "destination": "/api/main.py" }
  ]
}
