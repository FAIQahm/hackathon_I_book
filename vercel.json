{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "cleanUrls": true,
  "trailingSlash": false,
  "functions": {
    "api/main.py": {
      "runtime": "@vercel/python@4.0.0"
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/main.py"
    },
    {
      "source": "/health",
      "destination": "/api/main.py"
    },
    {
      "source": "/openapi.json",
      "destination": "/api/main.py"
    }
  ]
}
